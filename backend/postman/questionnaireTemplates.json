{
  "claimType": "vehicle",
  "claimOption": "accident",
  "title": "Vehicle Insurance - Accident Claim Questionnaire",
  "description": "Complete questionnaire for accident-related vehicle insurance claims",
  "sections": [
    {
      "title": "Accident Details",
      "description": "Provide key information about the accident",
      "order": 1,
      "questions": [
        {
          "questionId": "accident_date",
          "questionText": "Date of the accident",
          "questionType": "date",
          "isRequired": true,
          "order": 1,
          "validation": {
            "min": "1yAgo",
            "max": "today",
            "message": "Accident date must be within the past year and not in the future"
          }
        },
        {
          "questionId": "accident_time",
          "questionText": "Time of the accident (24h format, e.g., 14:30)",
          "questionType": "text",
          "isRequired": true,
          "order": 2,
          "validation": {
            "pattern": "^([01]?[0-9]|2[0-3]):[0-5][0-9]$",
            "message": "Please enter a valid time in HH:MM format"
          }
        },
        {
          "questionId": "accident_location",
          "questionText": "Location of the accident",
          "questionType": "text",
          "isRequired": true,
          "order": 3,
          "helpText": "Provide the street, city, or nearest landmark",
          "validation": {
            "pattern": "^[A-Za-z0-9 ,.()-]+$",
            "message": "Location can only include letters, numbers, spaces, and punctuation",
            "maxLength": 100
          }
        },
        {
          "questionId": "accident_description",
          "questionText": "Brief description of the accident",
          "questionType": "text",
          "isRequired": true,
          "order": 4,
          "validation": {
            "minLength": 10,
            "maxLength": 500,
            "message": "Description should be between 10 and 500 characters"
          }
        }
      ]
    },
    {
      "title": "Driver Information",
      "description": "Details about the driver at the time of the accident",
      "order": 2,
      "questions": [
        {
          "questionId": "driver_name",
          "questionText": "Driver's full name",
          "questionType": "text",
          "isRequired": true,
          "order": 1,
          "validation": {
            "pattern": "^[A-Za-z .'-]+$",
            "message": "Name can only include letters, spaces, and punctuation",
            "maxLength": 50
          }
        },
        {
          "questionId": "driver_license",
          "questionText": "Driver’s license number",
          "questionType": "text",
          "isRequired": true,
          "order": 2,
          "validation": {
            "pattern": "^[A-Z0-9-]+$",
            "message": "License number must contain only letters, numbers, or dashes",
            "maxLength": 20
          }
        },
        {
          "questionId": "relationship_to_policyholder",
          "questionText": "Relationship of driver to policyholder",
          "questionType": "select",
          "options": [
            "Policyholder",
            "Family member",
            "Authorized driver",
            "Other"
          ],
          "isRequired": true,
          "order": 3,
          "validation": {
            "message": "Please select a valid relationship"
          }
        }
      ]
    },
    {
      "title": "Damage & Injuries",
      "description": "Report any damages to the vehicle or injuries",
      "order": 3,
      "questions": [
        {
          "questionId": "vehicle_damage",
          "questionText": "Was the vehicle damaged?",
          "questionType": "boolean",
          "isRequired": true,
          "order": 1,
          "validation": {
            "message": "Please specify whether the vehicle was damaged"
          }
        },
        {
          "questionId": "damaged_part",
          "questionText": "Which part was most damaged?",
          "questionType": "select",
          "options": [
            "Front bumper",
            "Rear bumper",
            "Windshield",
            "Doors",
            "Engine",
            "Other"
          ],
          "isRequired": false,
          "order": 2,
          "validation": {
            "message": "Please select a damaged part if applicable"
          }
        },
        {
          "questionId": "injuries",
          "questionText": "Were there any injuries?",
          "questionType": "boolean",
          "isRequired": true,
          "order": 3,
          "validation": {
            "message": "Please specify if there were injuries"
          }
        }
      ]
    },
    {
      "title": "Pictures of the damaged vehicle",
      "description": "Upload documents to support your claim",
      "order": 4,
      "questions": [
        {
          "questionId": "documents",
          "questionText": "Upload accident-related documents",
          "questionType": "file",
          "isRequired": false,
          "order": 1,
